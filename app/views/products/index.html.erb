<p id="notice"><%= notice %></p>

<div id="explore_container">
  
  <div class="product_wrapper">
    

      <% @products.each do |p| %>
        <div class="product">
            <h2><%= p.name %></h2>
            <img src="<%= p.image.url(:medium) %>">
            <p><%= p.description %></p>
            <p><%= p.category%></p>
        </div>
      <% end %>
      
    <%= will_paginate %>
  </div>

  <div class="sidebar">
    <p>SideBar</p>
  </div>

</div>